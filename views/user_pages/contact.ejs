						<h4 class="mtext-105 cl2 txt-center p-b-30">
							Contact Admin
						</h4>

						<small id="mailer" style="color: red; display: none;">We will send the replay to your registered
							mail address</small>
						<div class="bor8 m-b-20 how-pos4-parent">
							<i class="fas fa-envelope how-pos4 pointer-none"></i>
							<input class="stext-111 cl2 plh3 size-116 p-l-62 p-r-30" id="email" type="email"
								name="email" placeholder="Your valid mail address"
								title="If your email is not valid, we will defaultly use your registered email.">
						</div>


						<small id="msger" style="color: red; display: none;">Please enter the message </small>
						<div class="bor8 m-b-30">
							<textarea id="message" minlength="5" class="stext-111 cl2 plh3 size-120 p-lr-28 p-tb-25"
								name="msg" placeholder="How Can We Help?"></textarea>
						</div>


						<button type="submit"
							class="flex-c-m stext-101 cl0 size-121 bg3 bor1 hov-btn3 p-lr-15 trans-04 pointer">
							Submit
						</button>
					</form>

				</div>

				<script>
					document.getElementById('contact').addEventListener('submit', function (event) {
						event.preventDefault();
						sendMessage();
					});

					async function sendMessage(event) {
						try {
							var email = document.getElementById('email').value.trim();
							var message = document.getElementById('message').value.trim();

							if (!message) {
								document.getElementById('msger').style.display = 'block';
								return;
							} else {
								document.getElementById('msger').style.display = 'none';
							}



							$.ajax({
								url: "/contact-admin",
								method: "POST",
								data: $('#contact').serialize(),
								success: res => {
									if (res.credential == false) {
										Swal.fire({
											icon: "error",
											title: "Oops...",
											text: "Invalid Credentials!",
											footer: '<a href="/login">Why lets login ?</a>'
										});
									} else if (res.succes == true) {
										Swal.fire({
											title: "Thanks!",
											text: "The message has been send, we will replay you as soon as possible !",
											icon: "success"
										});
										// Reset input values
										document.getElementById('email').value = '';
										document.getElementById('message').value = '';
									}
								},
								error: error => {
									Swal.fire({
										icon: "error",
										title: "Oops...",
										text: "Invalid Credentials!",
										footer: '<a href="/login"> lets register ?</a>'
									});
								}
							});

						} catch (error) {
							console.log('error while sending message: ', error.message);
						}
					}
				</script>


